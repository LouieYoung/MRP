<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.ItemMaster" table="t_itemmaster">
        <id name="id" column="id">
            <!--设置数据库表id增长
            native生成表id值就是主键自动增长
            -->
            <generator class="native"></generator>
        </id>
        <!--配置其他属性对应关系-->
        <property name="P_No" column="P_No"></property>
        <property name="LT" column="LT"></property>
        <property name="ST" column="ST"></property>
        <property name="SS" column="SS"></property>
        <property name="LLC" column="LLC"></property>
        <property name="LSR" column="LSR"></property>
        <property name="LS" column="LS"></property>

    </class>
</hibernate-mapping>
    