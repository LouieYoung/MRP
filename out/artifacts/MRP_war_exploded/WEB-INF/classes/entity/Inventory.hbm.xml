<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.Inventory" table="t_inventory">
        <id name="id" column="id">
            <!--设置数据库表id增长
            native生成表id值就是主键自动增长
            -->
            <generator class="native"></generator>
        </id>
        <!--配置其他属性对应关系-->
        <property name="P_No" column="P_No"></property>
        <property name="OH" column="OH"></property>
        <property name="AL" column="AL"></property>
        <property name="OO0" column="OO0"></property>
        <property name="OO1" column="OO1"></property>
        <property name="OO2" column="OO2"></property>
        <property name="OO3" column="OO3"></property>
        <property name="OO4" column="OO4"></property>
        <property name="OO5" column="OO5"></property>
        <property name="OO6" column="OO6"></property>
        <property name="OO7" column="OO7"></property>
        <property name="OO8" column="OO8"></property>
        <property name="OO9" column="OO9"></property>
        <property name="OO10" column="OO10"></property>
        <property name="OO11" column="OO11"></property>
        <property name="OO12" column="OO12"></property>

    </class>
</hibernate-mapping>
    